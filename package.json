    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/find": "^0.2.4",
