    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-n": "^17.21.3",
    "find": "^0.3.0",
    "fs-extra": "^11.3.1",
    "jiti": "^2.5.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
